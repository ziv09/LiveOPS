{
    "rules": {
        "liveops": {
            "v1": {
                "rooms": {
                    "$room": {
                        "$key": {
                            "state": {
                                ".read": "auth != null",
                                ".write": "auth != null"
                            }
                        }
                    }
                }
            },
            "v2": {
                "jaas": {
                    "rooms": {
                        "$room": {
                            "state": {
                                ".read": "auth != null",
                                "timeControl": {
                                    ".write": "auth != null"
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}